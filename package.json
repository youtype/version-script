  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/glob": "^0.4.0",
    "@actions/io": "^1.1.3",
    "@octokit/plugin-retry": "^4.1.3",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^6.15.0",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.7",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.5"
  }
}