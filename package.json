{
  "name": "version-script",
  "version": "0.0.1",
  "private": true,
  "description": "Version manager for mypy_boto3_builder ecosystem",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "build": "ncc build src/main.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youtype/version-script.git"
  },
